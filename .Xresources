URxvt*imLocale: en_US.UTF-8
URxvt*termName: rxvt-unicode-256color
URxvt*buffered: false

URxvt.intensityStyles: false
URxvt.font: xft:Fira Code Nerd Font:style=Retina:size=10   

URxvt.saveLines: 2000
URxvt.scrollBar: false
URxvt.cursorColor: white

URxvt*depth: 32
!URxvt*transparent: true
!URxvt*shading: 10

! special
URxvt*.foreground:   #ffffff
URxvt*.background:   #1F1F1F
URxvt*.cursorColor:  #ffffff

! black
*.color0:       #757575
*.color8:       #b8b8b8

! red
*.color1:       #ff5f5f
*.color9:       #d78787

! green
*.color2:       #a83334
*.color10:      #ffa095

! yellow
*.color3:       #d78787
*.color11:      #ff5f5f

! blue
*.color4:       #af5fd7
*.color12:      #dea3e5

! magenta
*.color5:       #ff87d7
*.color13:      #d7afaf

! cyan
*.color6:       #dea3e5
*.color14:      #af5fd7

! white
*.color7:       #b8b8b8
*.color15:      #757575
